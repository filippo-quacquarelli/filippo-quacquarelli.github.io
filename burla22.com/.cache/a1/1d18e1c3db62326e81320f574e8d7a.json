cutio.com/wiki/Printing_Tables\n   */\n  thead {\n    display: table-header-group;\n  }\n\n  tr,\n  img {\n    page-break-inside: avoid;\n  }\n\n  p,\n  h2,\n  h3 {\n    orphans: 3;\n    widows: 3;\n  }\n\n  h2,\n  h3 {\n    page-break-after: avoid;\n  }\n}\n\n\nbody {\n  font-family: 'Sue Ellen Francisco', cursive;\n  font-size: 22px;\n  line-height: 1.75;\n  background-color: #1f8297;\n}\n\nimg {\n  max-width: 100%;\n  height: auto;\n}\n\n.container {\n  max-width: 1920px;\n  margin: 0 auto 100px;\n  /* padding: 60px 15px; */\n}\n\n.big-image {\n  margin: 0 auto 60px;\n  display: block;\n}\n\nul {\n  margin: 0;\n  padding: 0;\n  list-style: none;\n}\n\n.menu li {\n  margin: 10px;\n  padding: 0;\n  max-width: 150px;\n  position: relative;\n}\n\n.menu ul {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: center;\n  align-items: flex-end;\n}\n\n.container-omino {\n  background-image: url(../img/Omino2.gif);\n  background-size: contain;\n  background-position: center;\n  height: 80vh;\n  background-repeat: no-repeat;\n}\n\n.note img {\n  max-width: 100px;\n}\n\n.menu .submenu {\n  position: absolute;\n  width: 100%;\n  flex-direction: column;\n  text-align: center;\n  display: none;\n  top: -90%;\n  background-color: #1f8297;\n}\n\n.menu .submenu li {\n  width: 100%;\n  margin: 0;\n}\n\n.menu .submenu li a {\n  font-size: 30px;\n  line-height: 1.2;\n  color: #000;\n}\n\n@media screen and (min-width: 1025px) {\n  .menu li:hover > .submenu {\n    display: block;\n  }\n}\n\n.menu .submenu.menu-open {\n  display: block;\n}"},"lineCount":null}},"error":null,"hash":"adedefd532577bf0d0d3a8eccc3f5031","cacheData":{"env":{}}}